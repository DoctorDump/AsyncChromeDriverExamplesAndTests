<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Chrome -->
    <add key="Driver" value="Zu.AsyncChromeDriver.SeleniumAdapter.WebDriverAdapter"/>
    <add key="Assembly" value="AsyncChromeDriver.SeleniumAdapter"/>
    <add key="DriverName" value="Chrome"/>
    <add key="RemoteCapabilities" value="chrome"/>

    <!-- Opera -->
    <!--add key="Driver" value="Zu.AsyncOperaDriver.SeleniumAdapter.WebDriverAdapter"/>
    <add key="Assembly" value="AsyncOperaDriver.SeleniumAdapter"/>
    <add key="DriverName" value="Chrome"/>
    <add key="RemoteCapabilities" value="chrome"/-->

    <!-- IE -->
    <!--add key="Driver" value="OpenQA.Selenium.IE.InternetExplorerDriver" />
    <add key="Assembly" value="WebDriver" />
    <add key="DriverName" value="IE"/>
    <add key="RemoteCapabilities" value="internet explorer"/-->

    <!-- Edge -->
    <!--add key="Driver" value="OpenQA.Selenium.Edge.EdgeDriver" />
    <add key="Assembly" value="WebDriver" />
    <add key="DriverName" value="Edge"/>
    <add key="RemoteCapabilities" value="MicrosoftEdge"/-->

    <!-- Firefox Release -->
      <!--add key="Driver" value="Zu.AsyncFirefoxDriver.SeleniumAdapter.WebDriverAdapter" />
    <add key="Assembly" value="AsyncFirefoxDriver.SeleniumAdapter" />
    <add key="DriverName" value="Firefox"/>
    <add key="RemoteCapabilities" value="firefox"/-->
    <!--add key="DriverStringArg" value="tests2" /-->

    <!-- Firefox Nightly -->
    <!--add key="Driver" value="OpenQA.Selenium.Firefox.NightlyFirefoxWebDriver" />
    <add key="Assembly" value="WebDriver.Firefox.Tests" />
    <add key="DriverName" value="Firefox"/>
    <add key="RemoteCapabilities" value="firefox"/-->

    <!-- Legacy Firefox Driver -->
    <!--add key="Driver" value="OpenQA.Selenium.Firefox.LegacyFirefoxWebDriver" />
    <add key="Assembly" value="WebDriver.Firefox.Tests" />
    <add key="DriverName" value="Firefox"/>
    <add key="RemoteCapabilities" value="firefox"/-->

    <!-- Safari -->
    <!--add key="Driver" value="OpenQA.Selenium.Safari.SafariDriver" />
    <add key="Assembly" value="WebDriver" />
    <add key="DriverName" value="Safari"/>
    <add key="RemoteCapabilities" value="safari"/-->

    <!-- Remote -->
    <!--add key="Driver" value="OpenQA.Selenium.Remote.TestInternetExplorerRemoteWebDriver" />
    <add key="Assembly" value="WebDriver.Remote.Tests" />
    <add key="DriverName" value="Remote"/>
    <add key="RemoteCapabilities" value="internet explorer"/>
    <add key="AutoStartRemoteServer" value="true"/-->

    <add key="Protocol" value="http"/>
    <add key="HostName" value="localhost"/>
    <add key="Port" value="2310"/>
    <add key="SecurePort" value="2410"/>
    <add key="Folder" value="HtmlForTests"/>
    <!-- <add key="StartTestWebServer" value="true"/>-->
     <add key="StartTestWebServer" value="false"/> 
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2"/></startup></configuration>
